<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Parallel Test Suite" parallel="tests" thread-count="10">
    <test name="Buying cheap computer chrome" parallel="tests" thread-count="10">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="tests.order.BuyingCheapComputerTest" />
            <class name="tests.global.footer.FooterTest" />
        </classes>
    </test>
    <test name="Buying standard computer chrome" parallel="tests" thread-count="10">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="tests.order.BuyingStandardComputerTest" />
        </classes>
    </test>
</suite>
